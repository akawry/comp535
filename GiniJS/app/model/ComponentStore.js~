Ext.define('GiniJS.models.Component', {
	extend: 'Ext.data.Model',
	idProperty: 'type',
	fields: [{
		name: 'type'
	}, {
		name: 'icon'
	}, {
		name: 'category'
	}]
});

Ext.create('Ext.data.Store', {
	model: 'GiniJS.models.Component',
	storeId: 'GiniJS.stores.ComponentStore',
	
	data: [{
		type: 'router', icon: 'app/resources/images/Router.gif', category: 'net'
	}, {
		type: 'uml', icon: 'app/resources/images/UML.gif', category: 'host'
	}]	
	
});